platform :ios, '13.0'

target 'mygodelement' do
  use_frameworks!

  # MatrixKit / MatrixSDK（你可能會用到）
  pod 'MatrixSDK', '~> 0.23.1'

  # Element iOS 常用套件（請依實際需求開啟）
  pod 'OLMKit', '~> 3.2.4'
  pod 'JitsiMeetSDK'  # 如果有用到視訊會議
  pod 'WebRTC'        # 若需要自建音視訊通話

  # 常見 UI 支援
  pod 'SDWebImage'
  pod 'IQKeyboardManagerSwift'
end